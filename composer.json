{
    "name": "v-sajidnawaz-t/laravel-api-responder",
    "description": "A Laravel package for standardized API responses.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sajid Nawaz",
            "email": "sajid@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "ApiResponder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApiResponder\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ApiResponder\\Providers\\ApiResponderServiceProvider"
            ]
        }
    }
}